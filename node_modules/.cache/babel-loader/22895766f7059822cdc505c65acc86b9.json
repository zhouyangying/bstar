ckTodo(id){\n      //遍历todo\n      this.todos.forEach((todo)=>{\n        if(todo.id===id) todo.done=!todo.done\n      })\n    },\n    deleteTodo(id){\n      this.todos = this.todos.filter((todo)=>{\n        return todo.id !== id\n      })\n    },\n    //全选或全不选\n    checkAllTodo(done){\n      this.todos.forEach((todo)=>{\n        todo.done = done\n      })\n    },\n    //清除所有已完成todo\n    clearAllTodo(){\n      this.todos = this.todos.filter((todo)=>{\n        return !todo.done\n      })\n    }\n  }\n};\n</script>\n\n<style  scoped>\n  .todo-container{\n    height: 300px;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n</style>>\n\n"]},"metadata":{},"sourceType":"module"}