{"ast":null,"code":"import MyHeader from \"./components/MyHeader.vue\";\nimport MyList from \"./components/MyList.vue\";\nimport MyFooter from \"./components/MyFooter.vue\";\nimport HelloWorld from \"./components/HelloWorld.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    MyHeader,\n    MyList,\n    MyFooter\n  },\n\n  data() {\n    return {\n      todos: [{\n        id: \"1\",\n        title: \"抽烟\",\n        done: true\n      }, {\n        id: \"2\",\n        title: \"喝酒\",\n        done: false\n      }, {\n        id: \"3\",\n        title: \"开车\",\n        done: true\n      }]\n    };\n  },\n\n  methods: {\n    //添加todo\n    addTodo(todoObj) {\n      this.todos.unshift(todoObj);\n    },\n\n    //勾选或取消勾选一个todo\n    checkTodo(id) {\n      //遍历todo\n      this.todos.forEach(todo => {\n        if (todo.id === id) todo.done = !todo.done;\n      });\n    },\n\n    deleteTodo(id) {\n      this.todos = this.todos.filter(todo => {\n        return todo.id !== id;\n      });\n    },\n\n    //全选或全不选\n    checkAllTodo(done) {\n      this.todos.forEach(todo => {\n        todo.done = done;\n      });\n    },\n\n    //清除所有已完成todo\n    clearAllTodo() {\n      this.todos = this.todos.filter(todo => {\n        return !todo.done;\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiBA,OAAOA,QAAP,MAAqB,2BAArB;AACA,OAAOC,MAAP,MAAmB,yBAAnB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IAAEL,QAAF;IAAYC,MAAZ;IAAoBC;EAApB,CAFC;;EAGbI,IAAI,GAAG;IACN,OAAO;MACNC,KAAK,EAAC,CACL;QAACC,EAAE,EAAC,GAAJ;QAAQC,KAAK,EAAC,IAAd;QAAmBC,IAAI,EAAC;MAAxB,CADK,EAEL;QAACF,EAAE,EAAC,GAAJ;QAAQC,KAAK,EAAC,IAAd;QAAmBC,IAAI,EAAC;MAAxB,CAFK,EAGL;QAACF,EAAE,EAAC,GAAJ;QAAQC,KAAK,EAAC,IAAd;QAAmBC,IAAI,EAAC;MAAxB,CAHK;IADA,CAAP;EAOD,CAXa;;EAYbC,OAAO,EAAC;IACN;IACAC,OAAO,CAACC,OAAD,EAAS;MACd,KAAKN,KAAL,CAAWO,OAAX,CAAmBD,OAAnB;IACD,CAJK;;IAKN;IACAE,SAAS,CAACP,EAAD,EAAI;MACX;MACA,KAAKD,KAAL,CAAWS,OAAX,CAAoBC,IAAD,IAAQ;QACzB,IAAGA,IAAI