{"ast":null,"code":"import MyHeader from \"./components/MyHeader.vue\";\nimport MyList from \"./components/MyList.vue\";\nimport MyFooter from \"./components/MyFooter.vue\";\nimport HelloWorld from \"./components/HelloWorld.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    MyHeader,\n    MyList,\n    MyFooter,\n    HelloWorld\n  },\n\n  data() {\n    return {\n      todos: [{\n        id: \"1\",\n        title: \"抽烟\",\n        done: true\n      }, {\n        id: \"2\",\n        title: \"喝酒\",\n        done: false\n      }, {\n        id: \"3\",\n        title: \"开车\",\n        done: true\n      }]\n    };\n  },\n\n  methods: {\n    //添加todo\n    addTodo(todoObj) {\n      this.todos.unshift(todoObj);\n    },\n\n    btnToTop() {\n      var timer = setInterval(function () {\n        let osTop = document.documentElement.scrollTop || document.body.scrollTop;\n        let isSpeed = Math.floor(-osTop / 5);\n        document.documentElement.scrollTop = document.body.scrollTop = osTop + isSpeed;\n        this.isTop = true;\n\n        if (osTop == 0) {\n          clearInterval(timer);\n        }\n      }, 10);\n    },\n\n    //勾选或取消勾选一个todo\n    checkTodo(id) {\n      //遍历todo\n      this.todos.forEach(todo => {\n        if (todo.id === id) todo.done = !todo.done;\n      });\n    },\n\n    deleteTodo(id) {\n      this.todos = this.todos.filter(todo => {\n        return todo.id !== id;\n      });\n    },\n\n    //全选或全不选\n    checkAllTodo(done) {\n      this.todos.forEach(todo => {\n        todo.done = done;\n      });\n    },\n\n    //清除所有已完成todo\n    clearAllTodo() {\n      this.todos = this.todos.filter(todo => {\n        return !todo.done;\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyBA,OAAOA,QAAP,MAAqB,2BAArB;AACA,OAAOC,MAAP,MAAmB,yBAAnB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IAAEL,QAAF;IAAYC,MAAZ;IAAoBC,QAApB;IAA8BC;EAA9B,CAFC;;EAGbG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QAAEC,EAAE,EAAE,GAAN;QAAWC,KAAK,EAAE,IAAlB;QAAwBC,IAAI,EAAE;MAA9B,CADK,EAEL;QAAEF,EAAE,EAAE,GAAN;QAAWC,KAAK,EAAE,IAAlB;QAAwBC,IAAI,EAAE;MAA9B,CAFK,EAGL;QAAEF,EAAE,EAAE,GAAN;QAAWC,KAAK,EAAE,IAAlB;QAAwBC,IAAI,EAAE;MAA9B,CAHK;IADF,CAAP;EAOD,CAXY;;EAYbC,OAAO,EAAE;IACP;IACAC,OAAO,CAACC,OAAD,EAAU;MACf,KAAKN,KAAL,CAAWO,OAAX,CAAmBD,OAAnB;IACD,CAJM;;IAKPE,QAAQ,GAAG;MACR,IAAIC,KAAI,GAAIC,WAAW,CAAC,YAAU;QACjC,IAAIC,KAAI,GAAIC,QAAQ,CAACC,eAAT,CAAyBC,SAAzB,IAAsCF,QAAQ,CAACG,IAAT,CAAcD,SAAhE;QACA,IAAIE,OAAM,GAAIC,IAAI,CAACC,KAAL,CAAW,CAACP,KAAD,GAAS,CAApB,CAAd;QACAC,QAAQ,CAACC,eAAT,CAAyBC,SAAzB,GAAqCF,QAAQ,CAACG,IAAT,CAAcD,SAAd,GAA0BH,KAAI,GAAIK,OAAvE;QACA,KAAKG,KAAL,GAAa,IAAb;;QACA,IAAGR,KAAK,IAAE,CAAV,EAAY;UACVS,aAAa,CAACX,KAAD,CAAb;QACF;MACA,CARsB,EAQrB,EARqB,CAAvB;IASF,CAfM;;IAgBP;IACAY,SAAS,CAACpB,EAAD,EAAK;MACZ;MACA,KAAKD,KAAL,CAAWsB,OAAX,CAAoBC,IAAD,IAAU;QAC3B,IAAIA,IAAI,CAACtB,EAAL,KAAYA,EAAhB,EAAoBsB,IAAI,CAACpB,IAAL,GAAY,CAACoB,IAAI,CAACpB,IAAlB;MACrB,CAFD;IAGD,CAtBM;;IAuBPqB,UAAU,CAACvB,EAAD,EAAK;MACb,KAAKD,KAAL,GAAa,KAAKA,KAAL,CAAWyB,MAAX,CAAmBF,IAAD,IAAU;QACvC,OAAOA,IAAI,CAACtB,EAAL,KAAYA,EAAnB;MACD,CAFY,CAAb;IAGD,CA3BM;;IA4BP;IACAyB,YAAY,CAACvB,IAAD,EAAO;MACjB,KAAKH,KAAL,CAAWsB,OAAX,CAAoBC,IAAD,IAAU;QAC3BA,IAAI,CAACpB,IAAL,GAAYA,IAAZ;MACD,CAFD;IAGD,CAjCM;;IAkCP;IACAwB,YAAY,GAAG;MACb,KAAK3B,KAAL,GAAa,KAAKA,KAAL,CAAWyB,MAAX,CAAmBF,IAAD,IAAU;QACvC,OAAO,CAACA,IAAI,CAACpB,IAAb;MACD,CAFY,CAAb;IAGD;;EAvCM;AAZI,CAAf","names":["MyHeader","MyList","MyFooter","HelloWorld","name","components","data","todos","id","title","done","methods","addTodo","todoObj","unshift","btnToTop","timer","setInterval","osTop","document","documentElement","scrollTop","body","isSpeed","Math","floor","isTop","clearInterval","checkTodo","forEach","todo","deleteTodo","filter","checkAllTodo","clearAllTodo"],"sourceRoot":"","sources":["/Users/zhouyang/zhouyang/test/pagetest/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"root\">\n    <div class=\"todo-container\">\n      <div class=\"todo-wrap\">\n        <MyHeader :addTodo=\"addTodo\"></MyHeader>\n        <MyList\n          :todos=\"todos\"\n          :checkTodo=\"checkTodo\"\n          :deleteTodo=\"deleteTodo\"\n        ></MyList>\n        <MyFooter\n          :todos=\"todos\"\n          :checkAllTodo=\"checkAllTodo\"\n          :clearAllTodo=\"clearAllTodo\"\n        >\n        </MyFooter>\n        <HelloWorld></HelloWorld>\n        \n      </div>\n    </div>\n    <button @click=\"btnToTop\" style=\"width:51px;height:51px\"></button>\n  </div>\n</template>\n\n<script>\nimport MyHeader from \"./components/MyHeader.vue\";\nimport MyList from \"./components/MyList.vue\";\nimport MyFooter from \"./components/MyFooter.vue\";\nimport HelloWorld from \"./components/HelloWorld.vue\";\n\nexport default {\n  name: \"App\",\n  components: { MyHeader, MyList, MyFooter, HelloWorld },\n  data() {\n    return {\n      todos: [\n        { id: \"1\", title: \"抽烟\", done: true },\n        { id: \"2\", title: \"喝酒\", done: false },\n        { id: \"3\", title: \"开车\", done: true },\n      ],\n    };\n  },\n  methods: {\n    //添加todo\n    addTodo(todoObj) {\n      this.todos.unshift(todoObj);\n    },\n    btnToTop() {\n       var timer = setInterval(function(){\n        let osTop = document.documentElement.scrollTop || document.body.scrollTop;\n        let isSpeed = Math.floor(-osTop / 5 );\n        document.documentElement.scrollTop = document.body.scrollTop = osTop + isSpeed; \n        this.isTop = true;\n        if(osTop==0){\n          clearInterval(timer);\n        }\n       },10)\n    },\n    //勾选或取消勾选一个todo\n    checkTodo(id) {\n      //遍历todo\n      this.todos.forEach((todo) => {\n        if (todo.id === id) todo.done = !todo.done;\n      });\n    },\n    deleteTodo(id) {\n      this.todos = this.todos.filter((todo) => {\n        return todo.id !== id;\n      });\n    },\n    //全选或全不选\n    checkAllTodo(done) {\n      this.todos.forEach((todo) => {\n        todo.done = done;\n      });\n    },\n    //清除所有已完成todo\n    clearAllTodo() {\n      this.todos = this.todos.filter((todo) => {\n        return !todo.done;\n      });\n    },\n  },\n};\n</script>\n\n<style  scoped>\n</style>>\n\n"]},"metadata":{},"sourceType":"module"}